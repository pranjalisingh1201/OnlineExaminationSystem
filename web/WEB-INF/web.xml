<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>HumanDetection</servlet-name>
        <servlet-class>HumanDetection</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveqp</servlet-name>
        <servlet-class>saveqp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteq</servlet-name>
        <servlet-class>deleteq</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteqp</servlet-name>
        <servlet-class>deleteqp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>result</servlet-name>
        <servlet-class>result</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HumanDetection</servlet-name>
        <url-pattern>/HumanDetection</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveqp</servlet-name>
        <url-pattern>/saveqp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteq</servlet-name>
        <url-pattern>/deleteq</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteqp</servlet-name>
        <url-pattern>/deleteqp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>result</servlet-name>
        <url-pattern>/result</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
